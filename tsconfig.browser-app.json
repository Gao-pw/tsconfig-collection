    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "composite": false,
    "tsBuildInfoFile": "./.build/tsconfig.browser-app.tsbuildinfo",
    "incremental": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "*": ["node_modules/*"]
    }
  }
}
